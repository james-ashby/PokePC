@{
    ViewData["Title"] = "Pokédex";
}
<h1>@ViewData["Title"]</h1>

<p>This page will show all of the Pokémon available.</p>
<div class="flextest container">
    @foreach (var entry in Model)
    {
        <div>
            <img class="pokemonImg" src="@entry.Image" alt="A picture of the Pokémon @entry.PokemonName">
            <p class="txt-pokename">@entry.PokemonName</p>
        </div>
    }
</div>

