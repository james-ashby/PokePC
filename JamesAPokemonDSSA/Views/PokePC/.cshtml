@{
    ViewData["Title"] = "Pokédex";
}
<h1>@ViewData["Title"]</h1>

<p>This page will show all of the Pokémon available.</p>
@foreach (var entry in Model)
{
    <div class="poke-info">
        <img class="pokemonImg" src="@entry.DexImage" alt="A picture of the Pokémon @entry.DexName">
        <p class="txt-pokename">@entry.DexName</p>
    </div>
}